{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "API Server for Interview AI Application",
        "title": "Interview AI API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
